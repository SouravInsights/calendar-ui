  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.13",
    "autoprefixer": "^10.0.4",
    "babel-loader": "8.1.0",
    "chokidar-cli": "^2.1.0",
    "cra-template": "1.1.1",
    "date-fns": "^2.16.1",
    "lodash": "^4.17.20",
